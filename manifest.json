{
  "manifest_version": 3,
  "name": "HTML/CSS Scraper",
  "version": "1.0",
  "description": "Scrapes HTML and CSS from the current page",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}